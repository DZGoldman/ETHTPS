  AverageTPS float
  ReadingsCount int
}

Ref: TPSData_Day.Provider > Providers.ID
Ref: TPSData_Day.Network > Networks.ID


Table TPSData_Week{
  ID int [pk, increment]
  Network int
  Provider int
  StartDate datetime
  AverageTPS float
  ReadingsCount int
}

Ref: TPSData_Week.Provider > Providers.ID
Ref: TPSData_Week.Network > Networks.ID

Table TPSData_Month{
  ID int [pk, increment]
  Network int
